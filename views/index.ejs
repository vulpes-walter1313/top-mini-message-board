<!DOCTYPE html>
<html>
  <%- include("./partials/head") %> 
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <a href="/new">Send a message</a>
    <div>
      <% messages.forEach(message => { %>
        <div class="message-display">
          <div class="name-message">
            <h2><%= message.username %></h2>
            <p><%= message.text %></p>
          </div>
          <p class="date-time"><%= message.createdat %></p>
        </div>
      <% }) %> 
    </div>
  </body>
</html>
